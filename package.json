		"@sveltejs/adapter-static": "1.0.0-next.43",
		"@sveltejs/kit": "1.0.0-next.481",
		"@types/aws-lambda": "^8.10.93",
		"@types/js-cookie": "^3.0.2",
		"@types/medium-editor": "^5.0.5",
		"@types/ws": "^8.5.3",
		"autoprefixer": "^10.4.4",
		"carbon-preprocess-svelte": "^0.8.2",
		"disqus-svelte": "^1.0.9",
