{
	"name": "@prognovel/frontend",
	"description": "Next-Generation WebNovel platform.",
	"version": "0.8.0",
	"type": "module",
	"scripts": {
		"populate": "node plugins/onbuild/populate/assets && node plugins/onbuild/populate/copy-assets",
		"dev:frontend": "vite dev --host",
		"dev:backend": "./bin/air serve",
		"dev": "run-p \"dev:*\"",
		"build": "npm run populate && vite build && node plugins/onbuild/finish",
		"preview": "vite preview",
		"test": "playwright test",
		"pocketbase:create-migration": "go run backend/main.go migrate create \"migration\"",
		"pocketbase:migrate": "go run backend/main.go migrate collections",
		"format": "prettier --write src/**/*.{ts,js,svelte} --plugin-search-dir=.",
		"sveltekit-upgrade": "pnpm add -D @sveltejs/kit@next @sveltejs/adapter-static@next svelte-preprocess@latest vite@latest"
	},
	"dependencies": {
		"@editorjs/editorjs": "^2.25.0",
