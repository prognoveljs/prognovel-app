		"@paypal/paypal-js": "^5.1.1",
		"@skeleton-elements/svelte": "^2.0.0",
		"carbon-components-svelte": "^0.59.0",
		"carbon-icons-svelte": "^11.3.0",
		"chalk": "^5.0.1",
		"crypto-es": "^1.2.7",
		"date-fns": "^2.28.0",
		"dotenv": "^8.6.0",
		"editorjs-html": "^3.4.2",
		"extract-zip": "^2.0.1",
		"front-matter": "^4.0.2",
		"globby": "^13.1.1",
		"hammerjs": "^2.0.8",
		"html-minifier": "^4.0.0",
		"idb-keyval": "6.1.0",
		"js-cookie": "^3.0.1",
		"js-crypto-aes": "^1.0.4",
		"ncp": "^2.0.0",
		"node-fetch": "^2.6.0",
		"pocketbase": "^0.8.0",
		"rxjs": "^7.5.5",
		"sharp": "^0.29.3",
		"short-uuid": "^4.2.0",
		"surrealdb.js": "^0.3.1",
		"svelte-feather-icons": "^4.0.0",
		"svelte-forms": "^2.3.1",
		"svelte-inview": "^3.0.0",
		"three": "^0.139.2",
		"web-vitals": "^1.1.2",
		"webfunding": "latest",
